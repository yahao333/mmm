{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "MiniMax Monitor 默认权限配置",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "log:default",
    "core:window:default",
    "core:window:allow-get-all-windows",
    "core:webview:allow-create-webview-window",
    "core:webview:allow-create-webview"
  ]
}
